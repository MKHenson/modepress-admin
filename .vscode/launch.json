{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Server",
      "type": "node",
      "protocol": "auto",
      "request": "launch",
      "stopOnEntry": false,
      "args": [
        "${workspaceRoot}/../../src/main.ts",
        "--config=${workspaceRoot}/../../config.json",
        "--numThreads=1",
        "--logging=true",
        "--inspect=true"
      ],
      "cwd": "${workspaceRoot}/../..",
      "preLaunchTask": "",
      "runtimeExecutable": null,
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": []
    },
    {
      "name": "Debug Tests",
      "type": "node",
      "protocol": "inspector",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "--compilers ts:ts-node/register",
        "-t",
        "100000",
        "./test/index.js",
        "--config=../../config.json",
        "--logging=false",
        "--runningTests=true",
        "--debug=true",
        "--clientsFolder=../../clients/",
        "--remotes.local.url=http://localhost:8001",
        "--remotes.local.path=../../clients/modepress-admin/test/media-uploads"
      ],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": []
    },
    {
      "name": "Watch Mode",
      "type": "node",
      "protocol": "auto",
      "request": "launch",
      "stopOnEntry": false,
      "args": [
        "./clients/modepress-admin/watch-server.js",
        "--config=./config.json",
        "--numThreads=1",
        "--runningTests=true",
        "--logging=true"
      ],
      "cwd": "${workspaceRoot}/../../",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": []
    }
  ]
}